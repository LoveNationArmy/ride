{
  "name": "@ride/web",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "proxy": "http://localhost:3001",
  "dependencies": {
    "@craco/craco": "^3.1.1",
    "@ride/stigma": "^0.1.0",
    "@ride/models": "^0.1.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "flow-runtime": "^0.17.0",
    "node-sass": "^4.10.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-easy-emoji": "^1.2.0",
    "react-scripts": "^2.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-plugin-flow-runtime": "^0.17.0",
    "dotenv": "^6.1.0",
    "dotenv-cli": "^1.4.0",
    "eslint-config-react-app": "^3.0.5",
    "fetch-mock": "^7.2.2",
    "now": "^12.1.3"
  },
  "scripts": {
    "deploy": "yarn clean && yarn build && now",
    "clean": "rm -rf build",
    "logs": "now logs -f",
    "dev": "dotenv -e .env.development yarn start",
    "start": "craco start",
    "build": "dotenv -e .env.production craco build",
    "test": "CI=true yarn test:run",
    "test:coverage": "yarn test:run --coverage --collectCoverageFrom=src/**/*.js",
    "test:watch": "yarn test:coverage --watch",
    "test:run": "craco test --setupTestFrameworkScriptFile=./test/setup.js --testMatch=**/*.test.js"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
